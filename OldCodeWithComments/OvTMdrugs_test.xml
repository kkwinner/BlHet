  <Plugin Name="Contact"> 
    
    <!-- add slight attraction for cancer to medium if want outer cells of spheroid to shed -->
    <Energy Type1="Medium" Type2="Medium">0</Energy> 
    <Energy Type1="IPfluid" Type2="Medium">10</Energy> 
    <Energy Type1="VisceralMesothelium" Type2="Medium">10</Energy> 
    <Energy Type1="PCancerGFP" Type2="Medium">10</Energy>
    <Energy Type1="PCancerRFP" Type2="Medium">10</Energy>
    <Energy Type1="QCancerGFP" Type2="Medium">10</Energy> 
    <Energy Type1="QCancerRFP" Type2="Medium">10</Energy>
    <Energy Type1="NCancer" Type2="Medium">3</Energy> 
    <Energy Type1="Endothelial" Type2="Medium">10</Energy> 
    <Energy Type1="LatentEndothelial" Type2="Medium">10</Energy> 
    <Energy Type1="VesselWall" Type2="Medium">10</Energy> 	  
    <Energy Type1="Adipocyte" Type2="Medium">10</Energy> 	  
    <Energy Type1="ECM" Type2="Medium">10</Energy> 	  
    <Energy Type1="SmoothMusc" Type2="Medium">10</Energy> 	  

    <!-- cancer more attracted to parietal mesothelium than medium -->
    <Energy Type1="IPfluid" Type2="IPfluid">0</Energy> 
    <Energy Type1="VisceralMesothelium" Type2="IPfluid">0</Energy> 
    <Energy Type1="PCancerGFP" Type2="IPfluid">10</Energy>
    <Energy Type1="PCancerRFP" Type2="IPfluid">10</Energy>
    <Energy Type1="QCancerGFP" Type2="IPfluid">0</Energy>        
    <Energy Type1="QCancerRFP" Type2="IPfluid">0</Energy>
    <Energy Type1="NCancer" Type2="IPfluid">3</Energy>        
    <Energy Type1="Endothelial" Type2="IPfluid">0</Energy>        
    <Energy Type1="LatentEndothelial" Type2="IPfluid">0</Energy>       
    <Energy Type1="VesselWall" Type2="IPfluid">0</Energy> 	  
    <Energy Type1="Adipocyte" Type2="IPfluid">0</Energy> 	  
    <Energy Type1="ECM" Type2="IPfluid">0</Energy> 	  
    <Energy Type1="SmoothMusc" Type2="IPfluid">0</Energy> 	  

    <!-- cancer more attracted to visceral mesothelium than parietal mesothelium -->  
    <!-- VM-VM: Keep VM together with attraction? -->
    <!-- PC-VM Jij: 
         6 = mesothelium pulled along w/chemotxing tumor
         7 = mesothelium closes behind invading tumor
         8 = hole remains in mesothelium after tumor invades -->
    <Energy Type1="VisceralMesothelium" Type2="VisceralMesothelium">0</Energy>
    <Energy Type1="PCancerGFP" Type2="VisceralMesothelium">20</Energy>
    <Energy Type1="PCancerRFP" Type2="VisceralMesothelium">20</Energy>
    <!-- QC and NC more neutral to VM than PC (a non-substantiated choice) -->
    <Energy Type1="QCancerGFP" Type2="VisceralMesothelium">10</Energy>         
    <Energy Type1="QCancerRFP" Type2="VisceralMesothelium">10</Energy>         
    <Energy Type1="NCancer" Type2="VisceralMesothelium">3</Energy>          
    <Energy Type1="Endothelial" Type2="VisceralMesothelium">22.5</Energy>          
    <Energy Type1="LatentEndothelial" Type2="VisceralMesothelium">10</Energy>          
    <Energy Type1="VesselWall" Type2="VisceralMesothelium">10</Energy> 
    <!-- invading cancer will push VM cells into adipocytes with them unless adipos. repel VM -->
    <Energy Type1="Adipocyte" Type2="VisceralMesothelium">20</Energy> 	  
    <!-- VM sticks to ECM less than sticks to self, so will not invade -->
    <Energy Type1="ECM" Type2="VisceralMesothelium">0</Energy> 	  
    <Energy Type1="SmoothMusc" Type2="VisceralMesothelium">10</Energy> 	  

    <!-- cancer sticks to self most strongly (compactness of spheroid = aggressive, images show nests or columns of invasive cells in omental fat) -->
    <!-- proliferating cancer sticks better to ECM than anything but PCancer and QCancerGFP -->
    <Energy Type1="QCancerGFP" Type2="QCancerGFP">0</Energy> 
    <Energy Type1="QCancerRFP" Type2="QCancerGFP">0</Energy> 
    <Energy Type1="PCancerGFP" Type2="QCancerGFP">0</Energy> 
    <Energy Type1="PCancerRFP" Type2="QCancerGFP">0</Energy> 
    <Energy Type1="NCancer" Type2="QCancerGFP">3</Energy> 
    <Energy Type1="Endothelial" Type2="QCancerGFP">0</Energy> 
    <Energy Type1="LatentEndothelial" Type2="QCancerGFP">0</Energy> 
    <Energy Type1="VesselWall" Type2="QCancerGFP">0</Energy> 	  
    <Energy Type1="Adipocyte" Type2="QCancerGFP">0</Energy> 	  
    <Energy Type1="ECM" Type2="QCancerGFP">0</Energy> 	  
    <Energy Type1="SmoothMusc" Type2="QCancerGFP">3</Energy> 	  

    <Energy Type1="QCancerRFP" Type2="QCancerRFP">0</Energy> 
    <Energy Type1="PCancerGFP" Type2="QCancerRFP">0</Energy> 
    <Energy Type1="PCancerRFP" Type2="QCancerRFP">0</Energy> 
    <Energy Type1="NCancer" Type2="QCancerRFP">3</Energy> 
    <Energy Type1="Endothelial" Type2="QCancerRFP">0</Energy> 
    <Energy Type1="LatentEndothelial" Type2="QCancerRFP">0</Energy> 
    <Energy Type1="VesselWall" Type2="QCancerRFP">0</Energy> 	  
    <Energy Type1="Adipocyte" Type2="QCancerRFP">0</Energy> 	  
    <Energy Type1="ECM" Type2="QCancerRFP">0</Energy> 	  
    <Energy Type1="SmoothMusc" Type2="QCancerRFP">3</Energy> 

    <!-- cancer sticks to self most strongly (compactness of spheroid = aggressive, images show nests or columns of invasive cells in omental fat) -->
    <!-- proliferating cancer sticks to ECM and A better than anything but PCancerGFP and QCancerGFP -->
    <Energy Type1="PCancerGFP" Type2="PCancerGFP">0</Energy> 
    <Energy Type1="PCancerRFP" Type2="PCancerGFP">0</Energy> 
    <Energy Type1="NCancer" Type2="PCancerGFP">3</Energy> 
    <Energy Type1="Endothelial" Type2="PCancerGFP">3</Energy> 
    <Energy Type1="LatentEndothelial" Type2="PCancerGFP">0</Energy> 
    <Energy Type1="VesselWall" Type2="PCancerGFP">0</Energy> 	  
    <Energy Type1="Adipocyte" Type2="PCancerGFP">3</Energy> 	  
    <Energy Type1="ECM" Type2="PCancerGFP">0</Energy> 	  
    <Energy Type1="SmoothMusc" Type2="PCancerGFP">3</Energy> 	  

    <Energy Type1="PCancerRFP" Type2="PCancerRFP">0</Energy> 
    <Energy Type1="NCancer" Type2="PCancerRFP">3</Energy> 
    <Energy Type1="Endothelial" Type2="PCancerRFP">3</Energy> 
    <Energy Type1="LatentEndothelial" Type2="PCancerRFP">0</Energy> 
    <Energy Type1="VesselWall" Type2="PCancerRFP">0</Energy> 	  
    <Energy Type1="Adipocyte" Type2="PCancerRFP">3</Energy> 	  
    <Energy Type1="ECM" Type2="PCancerRFP">0</Energy> 	  
    <Energy Type1="SmoothMusc" Type2="PCancerRFP">3</Energy> 	  

    <!-- Jij NC-all = 3; neutral to all cell types and medium -->
    <Energy Type1="NCancer" Type2="NCancer">3</Energy> 
    <Energy Type1="Endothelial" Type2="NCancer">3</Energy> 
    <Energy Type1="LatentEndothelial" Type2="NCancer">3</Energy> 
    <Energy Type1="VesselWall" Type2="NCancer">3</Energy> 	  
    <Energy Type1="Adipocyte" Type2="NCancer">3</Energy> 	  
    <Energy Type1="ECM" Type2="NCancer">3</Energy>
    <Energy Type1="SmoothMusc" Type2="NCancer">3</Energy> 	  

    <!-- adhesions of vessels to other components are unknown -->
    <Energy Type1="Endothelial" Type2="Endothelial">1</Energy> 
    <Energy Type1="LatentEndothelial" Type2="Endothelial">0</Energy> 
    <Energy Type1="VesselWall" Type2="Endothelial">0</Energy> 	  
    <Energy Type1="Adipocyte" Type2="Endothelial">13</Energy> 	  
    <Energy Type1="ECM" Type2="Endothelial">5.5</Energy> 	  
    <Energy Type1="SmoothMusc" Type2="Endothelial">2.5</Energy> 	  

    <Energy Type1="LatentEndothelial" Type2="LatentEndothelial">0</Energy> 
    <Energy Type1="VesselWall" Type2="LatentEndothelial">0</Energy> 	  
    <Energy Type1="Adipocyte" Type2="LatentEndothelial">13</Energy> 	  
    <Energy Type1="ECM" Type2="LatentEndothelial">1</Energy> 	  
    <Energy Type1="SmoothMusc" Type2="LatentEndothelial">0</Energy> 

    <!-- adhesions of vessels to other components are unknown -->	    
    <!-- VesselWall adheres strongly to keep vessel integrity (cells also frozen) -->	    
    <Energy Type1="VesselWall" Type2="VesselWall">0</Energy> 	  	  
    <Energy Type1="Adipocyte" Type2="VesselWall">13</Energy> 	  	  
    <Energy Type1="ECM" Type2="VesselWall">0</Energy> 	  	  
    <Energy Type1="SmoothMusc" Type2="VesselWall">0</Energy> 

    <!-- Adipocytes self-adhesion < cancer self-adhesion (images show nests or columns of invasive cells in omental fat) -->
    <Energy Type1="Adipocyte" Type2="Adipocyte">13</Energy> 	

    <!-- ECM sticks to Adipocyte, less than cancer (adipocytes sit in nets of collagen) -->
    <Energy Type1="ECM" Type2="Adipocyte">1</Energy> 	  	  
    <Energy Type1="SmoothMusc" Type2="Adipocyte">0</Energy>

    <!-- ECM-ECM adhesion unknown - set to allow PCancer to dissolve it when penetrating tissue -->
    <Energy Type1="ECM" Type2="ECM">1</Energy>
    <Energy Type1="SmoothMusc" Type2="ECM">0</Energy> 
    
    <Energy Type1="SmoothMusc" Type2="SmoothMusc">0</Energy>

  </Plugin> 




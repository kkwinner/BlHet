    <!-- spin flip attempts per Monte Carlo Step = flip2dimratio x # lattice sites --> 
    <Flip2DimRatio>0</Flip2DimRatio> <!-- set to zero since cells frozen -->

    <NeighborOrder>1</NeighborOrder> 

